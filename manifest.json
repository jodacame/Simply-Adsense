{
  "manifest_version": 2,

  "name": "Simply Advertising Earnings",
  "short_name": "Advertising Earnings",
  "description": "Simply Advertising Earnings is a easy and fast way to checking your adsense balance in Google Chrome.",
  "version": "1.0.9",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "identity"
  ],

"icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background":
  {
    "scripts":["jquery.min.js","moment.min.js","app.js","background.js"]
  },
  "oauth2": {
    "client_id": "1008257641717-rgnedflrlnpa8pus9cas5llh4t2uk9ru.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/adsense.readonly",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
